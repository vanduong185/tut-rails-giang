<p id="notice"><%= notice %></p>

<h1>User2s</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user2s.each do |user2| %>
      <tr>
        <td><%= user2.name %></td>
        <td><%= user2.email %></td>
        <td><%= link_to 'Show', user2 %></td>
        <td><%= link_to 'Edit', edit_user2_path(user2) %></td>
        <td><%= link_to 'Destroy', user2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User2', new_user2_path %>
